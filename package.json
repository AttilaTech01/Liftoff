  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/jest": "^29.5.10",
    "@types/jsonwebtoken": "^8.5.0",
    "concurrently": "^5.2.0",
    "cross-port-killer": "^1.2.1",
    "debug": "~2.6.9",
    "eslint": "^v6.8.0",
    "jest": "^29.7.0",
    "mock-require": "^3.0.1",
    "nodemon": "^2.0.22",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
